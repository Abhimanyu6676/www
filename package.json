{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "scripts": {
    "build": "gatsby build",
    "build_withPrefix": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "develop_lan": "gatsby develop -H 0.0.0.0 -p 8000",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop_lan",
    "serve_withPrefix": "gatsby serve -H 0.0.0.0 -p 8000 --prefix-paths",
    "serve": "gatsby serve -p 8000 -H 0.0.0.0",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "deploy": "gatsby build && gh-pages -d public",
    "deploy_withPrefix": "gatsby build --prefix-paths && gh-pages -d public",
    "publish": "gh-pages -d public",
    "ADD_UNIVERSALS_test": "git subtree add --prefix=@universals https://github.com/Abhimanyu6676/types-huelite main --squash",
    "ADD_UNIVERSALS": "git subtree add --prefix=src/@universals https://github.com/Abhimanyu6676/types-huelite main --squash",
    "PUSH_UNIVERSALS": "git subtree push --prefix=src/@universals https://github.com/Abhimanyu6676/types-huelite main --squash",
    "PULL_UNIVERSALS": "git subtree pull --prefix=src/@universals https://github.com/Abhimanyu6676/types-huelite main --squash"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.5",
    "@material-ui/core": "^4.11.2",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "axios": "^0.21.1",
    "babel-plugin-styled-components": "^1.12.0",
    "bootstrap": "^4.5.3",
    "expo": "^36.0.2",
    "gatsby": "^2.28.2",
    "gatsby-image": "^2.5.0",
    "gatsby-plugin-facebook-pixel": "^1.0.7",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-google-tagmanager": "^3.5.0",
    "gatsby-plugin-manifest": "^2.6.1",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-mdx": "^1.9.0",
    "gatsby-plugin-offline": "^3.4.0",
    "gatsby-plugin-polyfill-io": "^1.1.0",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^3.4.0",
    "gatsby-plugin-react-native-web": "^3.0.0-beta.7",
    "gatsby-plugin-sass": "^2.7.0",
    "gatsby-plugin-sharp": "^2.8.0",
    "gatsby-plugin-styled-components": "^3.9.0",
    "gatsby-plugin-use-query-params": "^1.0.1",
    "gatsby-remark-images": "^3.10.0",
    "gatsby-source-filesystem": "^2.5.0",
    "gatsby-transformer-sharp": "^2.6.0",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react-bootstrap": "^1.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
    "react-native-gesture-handler": "https://github.com/software-mansion/react-native-gesture-handler.git#95bfb4df7ce9b1e222d50ead99eee7e27cd79043",
    "react-native-web": "^0.11.7",
    "react-redux": "^7.2.4",
    "react-share": "^4.3.1",
    "react-spring": "^8.0.27",
    "redux": "^4.1.0",
    "styled-components": "^5.2.1",
    "use-query-params": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-native": "^0.63.37",
    "@types/styled-components": "^5.1.5",
    "babel-preset-expo": "^8.3.0",
    "gh-pages": "^3.1.0",
    "prettier": "2.1.2",
    "typescript": "^4.1.3"
  },
  "_peer-dep": {
    "material-ui/styles": ">=4.0.0",
    "popper.js": "^1.16.1",
    "jquery": "1.9.1 - 3",
    "webpack": "^3.0.0 || ^4.0.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/babel@7.0.4"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
