{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "description": "huelite website",
  "keywords": [
    "huelite"
  ],
  "scripts": {
    "develop": "gatsby develop -H 0.0.0.0 -p 8000",
    "start": "gatsby develop",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "_build": "gatsby build",
    "serve": "gatsby serve -H 0.0.0.0 -p 8000",
    "clean": "gatsby clean",
    "deploy": "gatsby build && scp -r ./public root@huelite.in:/var/www",
    "publish": "scp -r ./public root@huelite.in:/var/www"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@mui/icons-material": "^5.3.0",
    "@mui/material": "^5.2.8",
    "axios": "^0.25.0",
    "gatsby": "^4.5.2",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-image": "^2.5.1",
    "gatsby-plugin-manifest": "^4.5.1",
    "gatsby-plugin-material-ui": "^4.1.0",
    "gatsby-plugin-mdx": "^3.5.1",
    "gatsby-plugin-purgecss": "^6.1.2",
    "gatsby-plugin-react-helmet": "^5.5.0",
    "gatsby-plugin-sass": "^5.17.0",
    "gatsby-plugin-sharp": "^4.5.1",
    "gatsby-plugin-sitemap": "^5.5.0",
    "gatsby-plugin-use-query-params": "^1.0.1",
    "gatsby-remark-images": "^6.5.1",
    "gatsby-source-filesystem": "^4.5.1",
    "gatsby-transformer-sharp": "^4.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-spring": "^9.4.5",
    "sass": "^1.53.0",
    "use-query-params": "^1.2.3"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0"
  }
}
